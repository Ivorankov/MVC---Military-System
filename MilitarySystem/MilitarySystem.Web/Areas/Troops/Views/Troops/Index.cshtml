
@{

}
<nav class="light-blue darken-3 nav-text" id="tab-container">
    <div class="nav-wrapper valign-wrapper">
        <ul id="tab-bar">
            <li id="test"></li>
        </ul>
    </div>
</nav>

<div class="row ui-widget-content hidden" id="chat-box">
    <div class="col s12 m12">
        <div class="card blue lighten-3">
            <div class="card">
            </div>
            <div>
                <input type="range" min="0" max="9"/>
                <input type="text" id="message-to-server" placeholder="message" required="required" />
                <button id="send-message" class="btn">Send</button>
                <br />
            </div>
            <div class="card-content black-text human-text white" id="messages">

            </div>
        </div>
    </div>
</div>
<div id="user-id" class="hidden">@ViewBag.UserId</div>

@section Scripts{
    <script src="~/Scripts/jquery.signalR-2.2.0.min.js"></script>
    <script src="/signalr/hubs" type="text/javascript"></script>
    <script src="~/Scripts/app/chat.js" type="text/javascript"></script>
    <script src="~/Scripts/app/flexControlGenerator.js" type="text/javascript"></script>
    <script src="~/Scripts/app/googleMapsInitor.js" type="text/javascript"></script>
    <script src="~/Scripts/app/troops.js" type="text/javascript"></script>
    <script></script>
}
